<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.elegantelephant.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    

    <cache name="com.elegantelephant.social.SocialUserConnection"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Address"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Customer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Customer.vacations"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Customer.quoteRequests"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Passenger"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Passenger.vacations"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.QuoteRequest"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.QuoteRequest.passengers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.QuoteRequest.quotes"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Quote"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Quote.passengers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Vacation"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Vacation.quotes"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Vacation.passengers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Vendor"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Vendor.deals"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Deal"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.elegantelephant.domain.Card"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
